<% admin_breadcrumb link_to(plural_resource_name(Spree::ContactUsEmail), spree.admin_contact_us_emails_path) %>

<% admin_breadcrumb t('spree.new_contact_us_email') %>

<%= render partial: 'spree/shared/error_messages', locals: { target: @contact_us_email } %>

<%= form_for [:admin, @contact_us_email], method: :post, url: admin_contact_us_emails_path, html: { multipart: true } do |f| %>
  <fieldset data-hook="new_contact_us_email">
    <legend align="center"><%= t('spree.new_contact_us_email') %></legend>

    <%= render partial: 'form', locals: { f: f } %>

    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
